<h1>Users</h1>


<%=link_to "New User", new_user_url%>
<ul><% @users.each do |user| %>
<li><%= user.first_name  %>
<%= user.last_name %><br>
<%= user.email %><br>
<%= link_to image_tag(user.pic_url, width:200), user_url(user.id) %></li>
  <% if user.goals.present?%>
    <%= user.goals.first.name%>
    <% end %>
<% end %>
</ul>

